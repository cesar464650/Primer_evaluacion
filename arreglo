#include <stdio.h>
//Flores Castillo Cesar Roberto
int main(void){
	float x[13]={3.3, 4.5, 5.0, 3.4, 4.2, 5.3, 3.2, 4.3, 5.2, 7.1, 3.3, 4.8, 5.2};
	int i;	
	
	for (i=1; i<14; i++){
	if (x[i]<3.5)
	printf("Imperceptible, sin problema\n");
	
	if((x[i]>=3.5) && (x[i]<=5.4))
	printf("Sin problema, avisar\n");
	
	if((x[i]>=5.5) && (x[i]<=6.0))
	printf("Emitir alerta, desalojar\n");
	
	if((x[i]>=6.1) && (x[i]<=6.9))
	printf("Emitir alerta, enviar equipos de apoyo\n");
	
	if((x[i]>=7.0) && (x[i]<=7.9))
	printf("Emitir alerta, enviar equipos de rescate\n");
	
	if(x[i]>8)
	printf("Emergencia nacional, solicitar apoyo externo\n");
	
    }
	return 0;
}
